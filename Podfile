# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ForsquareSearch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ForsquareSearch
  pod 'Alamofire', '~> 4.5'
  pod 'AlamofireObjectMapper', '~> 5.0'
  pod 'RealmSwift'
  pod 'RxSwift',    '~> 4.0'

  target 'ForsquareSearchTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxTest',     '~> 4.0'
  end

  target 'ForsquareSearchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
